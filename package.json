{
  "name": "agriconnect",
  "version": "1.0.0",
  "description": "AI-powered multi-agent agricultural marketplace platform",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon src/server.js",
    "dev:client": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@langchain/core": "^0.1.52",
    "@langchain/openai": "^0.0.25",
    "@langchain/anthropic": "^0.1.37",
    "@langchain/google-genai": "^0.0.11",
    "langgraph": "^0.0.26",
    "langsmith": "^0.1.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "stripe": "^14.9.0",
    "algosdk": "^2.7.0",
    "axios": "^1.6.2",
    "ws": "^8.14.2",
    "uuid": "^9.0.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "node-cron": "^3.0.3",
    "elevenlabs": "^0.8.1"
  },
  "devDependencies": {
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "@vitejs/plugin-react": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "jsdom": "^23.0.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "agriculture",
    "ai",
    "multi-agent",
    "marketplace",
    "blockchain",
    "langchain",
